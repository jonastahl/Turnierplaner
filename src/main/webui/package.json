		"@typescript-eslint/parser": "^8.39.1",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/cli-plugin-typescript": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-vue": "^9.33.0",
		"prettier": "^3.6.2",
		"typescript": "^5.9.2",
