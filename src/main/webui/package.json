{
	"name": "turnierplaner",
	"version": "0.0.0",
	"scripts": {
		"build": "vue-tsc --noEmit && vite build",
		"dev": "vite",
		"preview": "vite preview --port 5050",
		"lint:validate": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx",
		"lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx --fix",
		"prettier:validate": "prettier . --check",
		"prettier:fix": "prettier . --write",
		"format:validate": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx && prettier . --check",
		"format:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.ts,.tsx,.js,.jsx --fix && prettier . --write"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/vue-query": "^5.67.1",
		"@tanstack/vue-query-devtools": "^5.67.1",
		"@vee-validate/yup": "^4.15.0",
		"@vue/cli": "^5.0.8",
		"axios": "^1.8.1",
		"keycloak-js": "^26.1.3",
		"material-symbols": "^0.29.0",
		"oidc-client-ts": "^3.1.0",
		"primeflex": "^3.3.1",
		"primeicons": "^7.0.0",
		"primevue": "^3.53.1",
		"sass": "^1.85.1",
		"sortablejs": "^1.15.6",
		"uuid": "^11.1.0",
		"vee-validate": "^4.15.0",
		"vue": "^3.5.13",
		"vue-axios": "^3.5.2",
		"vue-cal": "^4.10.2",
		"vue-i18n": "^11.1.1",
		"vue-password": "^3.1.0",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@types/sortablejs": "^1.15.8",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/cli-plugin-typescript": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-vue": "^9.33.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2",
		"unplugin-auto-import": "^19.1.1",
		"unplugin-vue-components": "^28.4.1",
		"vite": "^6.2.0",
