		"@vee-validate/yup": "^4.15.1",
		"@vue/cli": "^5.0.8 ",
		"axios": "^1.11.0",
		"keycloak-js": "^26.2.0",
		"material-symbols": "^0.34.1",
		"oidc-client-ts": "^3.3.0",
		"primeflex": "^3.3.1",
		"primeicons": "^7.0.0",
		"primevue": "^3.53.1",
		"sass": "^1.90.0",
		"sortablejs": "^1.15.6",
		"uuid": "^11.1.0",
		"vee-validate": "^4.15.1",
		"vue": "^3.5.18",
		"vue-axios": "^3.5.2",
		"vue-cal": "^4.10.2",
		"vue-i18n": "^11.1.11",
		"vue-password": "^3.1.0",
		"vue-router": "^4.5.1"
